/**index.wxss**/
.header{
  background-color: #5fca9c;
  width: 100%;
  height: 100rpx;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 10000;
}
.header .searchBar{
  width: 90%;
  height: 70rpx;
  padding-left: 20rpx;
  border-radius: 10rpx;
  margin: auto;
  position: relative;
  top: 50%;
  margin-top: -35rpx;
  background-color: rgba(255, 255, 255, 0.9);
  display: flex;
  align-items: center;
}
.header .searchBar text{
  font-size: 26rpx;
  margin-left: 10rpx;
  color: #b5b3b2;
}
.content{
  width: 100%;
  padding-top: 100rpx;
}
.bannerSwiper{
  width: 100%;
  height: 250rpx;
}
.bannerSwiper image{
  width: 100%;
  height: 100%;
}

.types{
  width: 100%;
  height: 300rpx;
  background-color: white;
  display: flex;
  flex-wrap: wrap;
}
.types .typeItem{
  width: 20%;
  height: 50%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.types .typeItem image{
  width: 120rpx;
  height: 100rpx;
}
.types .typeItem text{
  font-size: 26rpx;
}

.obSupermarket{
  width: 100%;
  height: 250rpx;
  background-color: white;
}
.obSupermarket .obHeader{
  width: 100%;
  height: 80rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 0 30rpx;
  background: linear-gradient(rgba(225, 236, 115, 0.3),#fff);
}
.obSupermarket .obHeader .left .logo{
  width: 140rpx;
  height: 36rpx;
  display: inline-block;
  vertical-align: middle;
  background-size: 100% 100%;
  background-image: url(data:image/png;base64,);
}
.obSupermarket .obHeader .left text{
  margin-left: 30rpx;
  display: inline-block;
  vertical-align: middle;
  color: #a5a5a5;
}
.obSupermarket .obHeader .right{
  color: black;
}

.obSupermarket .obContent{
  display: flex;
  flex-wrap: wrap;
  width: 100%;
  height: 170rpx;
}
.obSupermarket .obContent view{
  width: 25%;
  height: 50%;
  text-align: center;
}
.obSupermarket .obContent text{
  border: 1px solid #d2e32d;
  vertical-align: middle;
  display: inline-block;
  width: 80%;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  border-radius: 30rpx;
}

.headlines{
  width: 100%;
  height: 140rpx;
  background-color: white;
  margin-top: 20rpx;
  display: flex;
  align-items: center;
  justify-content: flex-start;
  padding: 0;
}
.headlines .logo{
  width: 160rpx;
  height: 120rpx;
  overflow: hidden;
  background-image: url(https://gw.alicdn.com/tfs/TB1Lm5gRXXXXXcOXFXXXXXXXXXX-200-150.png_160x160q75.jpg_.webp);
  background-size: 100%;
}
.headlines .headlineSwiper{
  flex:1;
  height: 100%;
}
.headlines .headlineSwiper .headline{
  width: 100%;
  height: 50%;
  display: flex;
  align-items: center;
}
.headlines .headlineSwiper .headline view{
  color:red;
  border: 1px solid red;
  border-radius: 6rpx;
  width: 60rpx;
}
.headlines .headlineSwiper .headline text{
  margin-left:10rpx;
  flex:1;
}

.barginPrice{
  width: 100%;
  height: 260rpx;
  background-color: white;
  margin-top: 20rpx;
  display: flex;
  align-items: center;
  padding: 0 30rpx;
}
.barginPrice .hotSale{
  width: 320rpx;
  height: 240rpx;
  position: relative;
  background-image: url(https://gw.alicdn.com/imgextra/TB14dARoCfD8KJjSszhSuvIJFXa.jpg_400x400q90.jpg_.webp);
  background-size: 100% 100%;
}
.barginPrice .hotSale .title{
  position: absolute;
  top: 0;
  left:0;
  width: 100%;
  height: 100%;
  background-image: url(https://gw.alicdn.com/tfs/TB1hEoaXMvGK1Jjy0FeXXXYupXa-518-384.png);
   background-size: 100% 100%;
}
.barginPrice .hotSale .name{
  position: absolute;
  top: 80rpx;
  left: 20rpx;
  color:white;
  width: 75%;
}
.barginPrice .hotSale .price{
  position: absolute;
  top: 170rpx;
  background-color: #ff5000;
  border-bottom-right-radius: 30rpx;
  color:white;
  width: 60%;
  padding-left: 20rpx;
  height: 50rpx;
  line-height: 45rpx;
}
.barginPrice .ticket{
  width: 170rpx;
  height: 240rpx;
  position: relative;
  background-size: 100% 100%;
  margin-left: 20rpx;
}
.barginPrice .ticket1{
  background-image: url(https://gw.alicdn.com/tfs/TB1eGFZSFXXXXcdaXXXXXXXXXXX-251-384.png_400x400q90.jpg_.webp);
}
.barginPrice .ticket2{
  background-image: url(https://gw.alicdn.com/tfs/TB1jtUISFXXXXcrXFXXXXXXXXXX-249-384.png_400x400q90.jpg_.webp);
}
.barginPrice .ticket .title{
  position: absolute;
  top: 80rpx;
  left: 10rpx;
  font-size: 24rpx;
}
.barginPrice .ticket .tips{
  position: absolute;
  top: 150rpx;
  left: 10rpx;
  color:#ff5000;
  font-size: 24rpx;
}

.section{
  margin-top: 20rpx;
  width: 100%;
  background-color: white;    
}
.section .contentWrapper{
  padding: 0 20rpx;
}
.firstStation .contentWrapper .hot{
  width: 100%;
  height: 260rpx;
  display: flex;
}
.firstStation .contentWrapper .hot .hotLeft{
  width: 30%;
  height: 100%;
  display: flex;
  flex-direction: column;
  background-color: #f2f3f4;
  padding-left: 10rpx;
}
.firstStation .contentWrapper .hot .hotLeft .tips{
  background-color: #ffc900;
  color: black;
  width: 80rpx;
  height: 60rpx;
  text-align: center;
  line-height: 60rpx;
}
.firstStation .contentWrapper .hot .hotLeft .text{
  font-size: 32rpx;
  line-height: 1.2em;
  margin-top: 20rpx;
  color: #1e1e1e;
}
.firstStation .contentWrapper .hot .hotLeft .people{
  margin-top: 10rpx;
  padding-top: 10rpx;
  border-top: 1px solid #ffc900;
  width: 120rpx;
}
.firstStation .contentWrapper .hot .hotRight{
  width: 70%;
  height: 100%;
}
.firstStation .contentWrapper .stations{
  margin-top: 10rpx;
  display: flex;
  justify-content: space-between;
}
.firstStation .contentWrapper .stations .stationItem{
  width: 230rpx;
  height: 280rpx;
}
.firstStation .contentWrapper .stations .stationItem .stationImg{
  position: relative;
  width: 100%;
  height: 140rpx;
}
.firstStation .contentWrapper .stations .stationItem .stationImg image{
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
}
.firstStation .contentWrapper .stations .stationItem .stationImg view{
  position: absolute;
  bottom: 0;
  left: 0;
  color: white;
  background-color: rgba(0, 0, 0, 0.5);
  text-align: center;
  height: 40rpx;
  line-height: 40rpx;
  padding: 0 20rpx;
}
.firstStation .contentWrapper .stations .stationItem .stationTitle{
  width: 100%;
  margin-top: 10rpx;
  padding-right: 10rpx;
  color:black;
}

.randomPlace .contentWrapper{
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
}
.randomPlace .placeItem{
  width: 230rpx;
  height: 280rpx;
}
.randomPlace .placeItem image{
  width: 100%;
  height: 140rpx;
}
.randomPlace .placeItem .title{
  color: black;
  margin-top: 10rpx;
  font-size: 32rpx;
  display: block;
}
.randomPlace .placeItem .tips{
  margin-top: 10rpx;
  display: block;
}

.hotPlace .contentWrapper{
  display: flex;
  justify-content: space-between;
  height: 300rpx;
}
.hotPlace .contentWrapper .left{
  width: 33%;
  height: 100%;
}
.hotPlace .contentWrapper .right{
  height: 100%;
  width: 66%;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  align-content: space-between;
}
.hotPlace .contentWrapper .right .placeItem{
  width: 49%;
  height: 49%;
}
.hotPlace .contentWrapper .placeItem{
  position: relative;
}
.hotPlace .contentWrapper .placeItem image{
  position: absolute;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
}
.hotPlace .contentWrapper .placeItem .bg{
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
  background: linear-gradient(180deg,transparent 0,rgba(0,0,0,.3) 50%);
}
.hotPlace .contentWrapper .placeItem .placeName{
  color: white;
  position: absolute;
  top: 16rpx;
  left: 10rpx;
  font-size: 38rpx;
}
.hotPlace .contentWrapper .placeItem .placeNum{
  color: white;
  position: absolute;
  top: 80rpx;
  left: 10rpx;
}

.like .contentWrapper{
  background-color: #f6f6f6;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
}
.like .contentWrapper .likeItem{
  width: 49%;
  height: 540rpx;
  background-color: white;
  margin-bottom: 20rpx;
}
.like .contentWrapper .likeItem .imgBox{
  position: relative;
  width: 100%;
  height: 350rpx;
}
.like .contentWrapper .likeItem .imgBox image{
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
}
.like .contentWrapper .likeItem .imgBox .type{
  position: absolute;
  top: 10rpx;
  left: 20rpx;
  color:white;
  background-color: rgba(0, 0, 0, 0.8);
  padding: 10rpx;
  border-radius: 20rpx;
  border-top-right-radius: 0;
}
.like .contentWrapper .likeItem .title{
  color:black;
  margin-top: 20rpx;
  padding: 0 10rpx;
}
.like .contentWrapper .likeItem .info{
  margin-top: 20rpx;
  padding: 0 10rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.like .contentWrapper .likeItem .info .price{
  color:#ff5000;
  font-size: 36rpx;
}
.like .contentWrapper .likeItem .info .num{
  font-size: 26rpx;
}